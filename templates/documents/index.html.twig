{% extends 'base.html.twig' %}

{% block title %}Документы{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('documents') }}
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('documents') }}
{% endblock %}

{% block body %}
    <div class="page">
        {% include('block/aside.html.twig') %}
        <div class="page-wrapper">
            {% include 'block/header.html.twig'
                with {
                header: 'Документы',
            }
            %}
            <div class="vh-100">

                <div class="top-bl">
                    {{ include('documents/elements/table.html.twig') }}
                </div>
                <div class="bottom-bl">
                    {{ include('documents/elements/table.html.twig') }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
